{
  "name": "retail",
  "version": "1.0.0",
  "description": "A basic template that consists of the essential elements that are required to start building a React application using Redux",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --env.env=dev",
    "build:dev:watch": "npm run build:dev -- --watch",
    "build:dev:bundleanalyze": "npm run build:dev -- --env.addons=bundleanalyze",
    "build:dev:bundlebuddy": "npm run build:dev -- --env.addons=bundlebuddy",
    "serve:dev": "webpack-dev-server --env.env=dev --hot",
    "serve:dev:dashboard": "webpack-dashboard -- webpack-dev-server --hot --env.env=dev --env.addons=dashboard",
    "build:prod": "npm run build -- -p --env.env=prod --bail --display=errors-only",
    "build:prod:watch": "npm run build:prod -- --watch",
    "build:prod:bundleanalyze": "npm run build:prod -- --env.addons=bundleanalyze",
    "build:prod:bundlebuddy": "npm run build:prod -- --env.addons=bundlebuddy",
    "serve:prod": "npm run build:prod && live-server ./public",
    "lint": "eslint .; exit 0",
    "lint:fix": "eslint . --fix; exit 0",
    "start": "npm run serve:dev",
    "test": "echo \"No tests available\"; exit 0",
    "webpack-defaults": "webpack-defaults"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "aspnet-webpack": "^3.0.0",
    "aspnet-webpack-react": "^4.0.0",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.9",
    "eslint": "^4.18.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.6.1",
    "live-server": "^1.2.1",
    "node-sass": "^4.12.0",
    "react-hot-loader": "3.1.3",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^0.20.2",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-cli": "^3.3.11",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@sentry/react": "^6.13.3",
    "@sentry/tracing": "^6.13.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.0",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "date-fns": "^2.14.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fig-webpack-progress": "^0.0.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodelist-foreach-polyfill": "^1.2.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.0",
    "qrcode": "^1.5.0",
    "react": "^16.13.1",
    "react-datepicker": "^4.5.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.5.0",
    "react-mini-chart": "^0.1.3",
    "react-multi-carousel": "^2.6.5",
    "react-redux": "^7.2.6",
    "react-responsive-carousel": "^3.2.21",
    "react-router": "^5.2.1",
    "react-router-dom": "^4.2.2",
    "react-select": "^5.1.0",
    "react-sortable-hoc": "^2.0.0",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^3.0.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "video.js": "^7.15.4",
    "webpack-dev-middleware": "^5.2.1",
    "whatwg-fetch": "^3.0.0",
    "zxcvbn": "^4.4.2"
  }
}
